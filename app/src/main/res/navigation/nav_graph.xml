            app:destination="@id/parentHomeFragment" />
    </fragment>
    <dialog
        android:id="@+id/gradingSuccessDialogFragment"
        android:name="com.fortfighter.ui.dialog.GradingSuccessDialogFragment"
        android:label="GradingSuccessDialogFragment" />
