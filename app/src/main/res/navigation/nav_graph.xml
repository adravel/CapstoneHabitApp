            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/rankingFragment"
        android:name="com.fortfighter.ui.fragment.RankingFragment"
        android:label="RankingFragment"
        tools:layout="@layout/fragment_ranking"/>
