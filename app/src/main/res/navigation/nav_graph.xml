            android:id="@+id/action_parentHomeFragment_to_rankingFragment"
            app:destination="@id/rankingFragment" />
        <action
            android:id="@+id/action_parentHomeFragment_to_history"
            app:destination="@id/historyFragment" />
    </fragment>
